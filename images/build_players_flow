digraph {
	A [label="Agent metadata list
(list of dicts: {name, gender, goal, ...})"]
	B [label=create_player_configs]
	C [label="AgentConfig list"]
	D [label=build_players]
	E [label="For each AgentConfig in player_configs"]
	F [label=build_agent]
	F1 [label="Generate memories
(make_memories)"]
	F2 [label="Define agent components"]
	F3 [label="Instantiate agent (BasicAgent)"]
	F4 [label="Return agent instance and agent memory"]
	G [label="Append agent to players list"]
	H [label="Store memory in memories dict"]
	I [label="Return:
Players list
Memories dict"]
	A -> B
	B -> C
	C -> D
	D -> E
	E -> F [label="Parallel loop"]
	F -> F1
	F1 -> F2
	F2 -> F3
	F3 -> F4
	F4 -> G
	F4 -> H
	G -> E [style=dashed]
	H -> E [style=dashed]
	G -> I
	H -> I
}
